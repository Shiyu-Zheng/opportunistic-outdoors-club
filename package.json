  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.10"
  }
}
